{
    "name": "experemental-coin",
    "version": "1.0.0",
    "description": "experemental-coin",
    "main": "index.js",
    "scripts": {
        "compile": "npm run clean && cd node_modules/.bin && truffle compile",
        "migrate": "npm run clean && cd node_modules/.bin && truffle migrate",
        "clean": "rm -rf ./build",
        "attach": "geth attach rpc:http://localhost:8545",
        "mist": "mist --rpc http://localhost:8545 --swarmurl \"null\"",
        "testrpc": "testrpc --secure -u 0 -u 1 -u 2 -u 3 -u 4 -u 5 -u 6 -u 7 -u 8 -u 9",
        "geth_local": "cd \"c:/Program Files/Geth/\" && geth --rpc --dev --rpcapi admin,debug,miner,shh,txpool,personal,eth,net,web3 console",
        "test": "node_modules/.bin/truffle test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DmitryRomaniuk/experemental-coin.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/DmitryRomaniuk/experemental-coin/issues"
    },
    "homepage": "https://github.com/DmitryRomaniuk/experemental-coin#readme",
    "devDependencies": {
        "truffle": "^3.4.9",
        "zeppelin-solidity": "1.2.0"
    }
}
